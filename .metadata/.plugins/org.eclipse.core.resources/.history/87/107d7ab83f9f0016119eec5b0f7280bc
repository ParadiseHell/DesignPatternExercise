package com.chengtao.impl;

public interface BaseImpl extends Cloneable{
	/**
	 * 计算方法
	 * @author ChengTao
	 *
	 */
	enum CalculateType{
		/**
		 * 加法
		 */
		ADD,
		/**
		 * 减法
		 */
		SUB,
		/**
		 * 乘法
		 */
		MULTI,
		/**
		 * 除法
		 */
		DIV
	}
	/**
	 * 计算
	 * @param obj
	 * @param type
	 * @return
	 */
	<T extends Object> T calulate(T obj,CalculateType type);
}
