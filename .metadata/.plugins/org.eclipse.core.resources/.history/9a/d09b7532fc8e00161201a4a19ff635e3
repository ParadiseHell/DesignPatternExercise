package com.ct.factory.base;

import com.ct.factory.impl.TVFactory;

/**
 * 抽象电视工厂类
 * @author ChengTao
 *
 */
public abstract class AbstractTVFctory implements TVFactory{
	//-----------------变量
	//工厂名字
	protected String factoryName;
	//生产电视的字符串
	protected final static String productStr = " create a TV";
	/**
	 * 设置工厂的名字
	 */
	public abstract void setFactoryName();
}
