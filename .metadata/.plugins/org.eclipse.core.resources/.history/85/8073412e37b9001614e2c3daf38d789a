package com.chengtao.main;

import com.chengtao.utils.FileUtils;
import com.chengtao.utils.NodeUtils;

public class Composition {

	public static void main(String[] args) {
		CompositeIterator iterator = NodeUtils.getNodeIterator(FileUtils.readTreeInfoFromFile("TreeInfo.txt"));
		while (iterator.hasNext()) {
			System.out.println(iterator.next().getSelfName());
		}
	}
}
